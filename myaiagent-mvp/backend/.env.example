# REQUIRED - Core Security Secrets
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here_32_bytes_hex
CSRF_SECRET=your_csrf_secret_here

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/myaiagent_db

# AI/LLM API Keys
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Google OAuth Configuration (for Google Login)
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:3000/auth/google/callback

# Google Search Configuration
GOOGLE_SEARCH_API_KEY=your_google_search_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_google_search_engine_id_here

# Federal Contracting APIs
SAM_GOV_API_KEY=your_sam_gov_api_key_here

# Google Vertex AI Configuration
VERTEX_AI_PROJECT_ID=your_vertex_ai_project_id

# Server Configuration
NODE_ENV=development
PORT=3000

# Frontend Configuration
FRONTEND_URL=http://localhost:5000

# File Upload Configuration
UPLOAD_DIR=./uploads

# Session Configuration (in hours)
SESSION_DURATION_HOURS=24

# CORS Configuration (comma-separated origins)
CORS_ORIGINS=http://localhost:5000,http://localhost:3000

# Replit Development (optional)
REPLIT_DEV_DOMAIN=

# Debug Options
DEBUG_QUERIES=false
